{
	"name": "cisco-webex-websocket",
	"version": "1.1.1",
	"main": "dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitfocus/companion-module-cisco-webex-websocket.git"
	},
	"license": "MIT",
	"scripts": {
		"prepare": "husky install",
		"dev": "yarn build:watch",
		"build": "rimraf dist && yarn build:main",
		"build:main": "tsc",
		"build:watch": "tsc --watch",
		"lint:raw": "eslint --ext .ts --ext .js --ignore-pattern dist --ignore-pattern pkg",
		"lint": "yarn lint:raw .",
		"postinstall": "rimraf node_modules/ssh2/lib/protocol/crypto/build node_modules/cpu-features"
	},
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		],
		"*.{ts}": [
			"yarn lint --fix --",
			"git add"
		]
	},
	"prettier": "@companion-module/tools/.prettierrc.json",
	"dependencies": {
		"@companion-module/base": "~1.4.1",
		"jsxapi": "^5.1.1",
		"ws": "^8.13.0"
	},
	"devDependencies": {
		"@companion-module/tools": "^1.3.1",
		"@types/node": "^18.16.18",
		"@types/ws": "^8.5.5",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"rimraf": "^5.0.1",
		"typescript": "~5.0"
	}
}
